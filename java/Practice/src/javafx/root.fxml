<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.input.KeyCodeCombination?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="javafx.Controller">
   <top>
      <VBox prefHeight="30.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <MenuBar prefHeight="30.0" prefWidth="600.0">
              <menus>
                <Menu mnemonicParsing="false" text="파일(F)">
                  <items>
                    <MenuItem mnemonicParsing="false" text="새로 만들기(N)" onAction="#newview1"  >
	                    <accelerator>
	                    	<KeyCodeCombination control="DOWN" code="N" meta="UP" alt="UP" shift="UP" shortcut="UP" />                  
	                    </accelerator>
                    </MenuItem>
					<MenuItem mnemonicParsing="false" text="새 창(W)" onAction="#newview2" >
                          <accelerator>
                    		<KeyCodeCombination control="DOWN" shift="UP" code="W" meta="UP" alt="UP" shortcut="UP" />
                          </accelerator>
                    </MenuItem>
                        <MenuItem mnemonicParsing="false" text="열기(O)" onAction="#open">
                          <accelerator>
                    <KeyCodeCombination control="DOWN" code="O" meta="UP" alt="UP" shift="UP" shortcut="UP" />
                    	  </accelerator>
                        </MenuItem>
                        <MenuItem mnemonicParsing="false" text="저장(S)" onAction="#save">
                          <accelerator>
                    <KeyCodeCombination control="DOWN" code="S" meta="UP" alt="UP" shift="UP" shortcut="UP" /> 
                   		  </accelerator>
                        </MenuItem>
                        <MenuItem mnemonicParsing="false" text="다른 이름으로 저장(A)" onAction="#othersave" >
                          <accelerator>
                  				 <KeyCodeCombination control="DOWN" shift="DOWN" code="S" meta="UP" alt="UP" shortcut="UP" />                 
                   		  </accelerator>
                                    </MenuItem>
                        <MenuItem mnemonicParsing="false" text="페이지 설정(U)" onAction="#option" >
                          <accelerator>
                   			 <KeyCodeCombination code="U" meta="UP" alt="UP" shift="UP" control="UP" shortcut="UP" />                   
                  		  </accelerator>               
                        </MenuItem>
                        <MenuItem mnemonicParsing="false" text="인쇄(P)" onAction="#print">
                          <accelerator>
                   				 <KeyCodeCombination control="DOWN" code="P" meta="UP" alt="UP" shift="UP" shortcut="UP" />
                   		  </accelerator>
                    
                        </MenuItem>
                        <MenuItem mnemonicParsing="false" text="끝내기(X)" onAction="#quit" >
                          <accelerator>
                   			 <KeyCodeCombination code="X" meta="UP" alt="UP" shift="UP" control="DOWN" shortcut="UP" />                   
                  		  </accelerator>                
                        </MenuItem>     
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="편집(E)">
                  <items>
                    <MenuItem mnemonicParsing="false" text="실행취소(U)" />
                      <MenuItem mnemonicParsing="false" text="잘라내기(T)" />
                        <MenuItem mnemonicParsing="false" text="복사(C)" />
                        <MenuItem mnemonicParsing="false" text="붙여넣기(P)" />
                        <MenuItem mnemonicParsing="false" text="삭제(L)" />
                        <MenuItem mnemonicParsing="false" text="Bing으로 검색(S)" />
                        <MenuItem mnemonicParsing="false" text="찾기(F))" />
                        <MenuItem mnemonicParsing="false" text="다음 찾기(N)" />
                         <MenuItem mnemonicParsing="false" text="이전 찾기(V)" />
                        <MenuItem mnemonicParsing="false" text="바꾸기(R)" />
                        <MenuItem mnemonicParsing="false" text="이동(G))" />
                        <MenuItem mnemonicParsing="false" text="모두 선택(A)" />
                        <MenuItem mnemonicParsing="false" text="시간/날짜(D)" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="서식(O)">
                  <items>
                    <MenuItem mnemonicParsing="false" text="자동 줄 바꿈(W)" />
                    <MenuItem mnemonicParsing="false" text="글꼴(F)" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="보기(V)">
                    <items>
                      <MenuItem mnemonicParsing="false" text="확대하기/축소하기" />
                      <MenuItem mnemonicParsing="false" text="상태 표시줄(S)" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="도움말(H)">
                    <items>
                      <MenuItem mnemonicParsing="false" text="도움말 보기(H)" />
                      <MenuItem mnemonicParsing="false" text="피드백 보내기(F)" />
                      <MenuItem mnemonicParsing="false" text="메모장 정보(A)" />
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
         </children>
      </VBox>
   </top>
   <center>
      <TextArea prefHeight="74.0" prefWidth="600.0" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <ScrollBar BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <ScrollBar orientation="VERTICAL" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
